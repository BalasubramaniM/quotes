{
	"manifest_version": 2,
	"name": "Quotes",
	"description": "Simplistic quotes app for your Chrome browser.",
	"version": "1.1.1",
	"author": "Balasubramani M",
	"browser_action": {
		"default_title": "Quotes",
		"default_icon": "src/assets/favicon.png"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": ["activeTab"]
}
